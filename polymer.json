{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/add-book.html",
    "src/book-form.html",
    "src/book-info.html",
    "src/edit-book.html",
    "src/form-line.html",
    "src/formline-select.html",
    "src/list-item.html",
    "src/my-view404.html",
    "src/scrollable-list.html",
    "src/search-book.html",
    "src/search-form.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
